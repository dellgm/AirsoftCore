@page
@model AirsoftCore.WebUI.Pages.Categories.CreateModel
@{
    ViewData["Title"] = "Create";
}

<h2>Add New Brand</h2>

<div class="row">
    <div class="col-md-12">
        <form method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label asp-for="Brand.Descr" class="control-label"></label>
                <input asp-for="Brand.Descr" class="form-control" />
                <span asp-validation-for="Brand.Descr" class="text-danger"></span>
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-outline-primary">Create</button>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @await Html.PartialAsync("_EditorScript")

}